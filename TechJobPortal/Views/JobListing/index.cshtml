@model IEnumerable<TechJobPortal.Models.JobListing>
@{
    ViewData["Title"]="Jop List";
}

<h2> @ViewData["Title"] </h2>


<table class="table">
    <thead>
        <tr>
            <th>Job Title</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var job in Model)
        {
            <tr>
                <td>@job.title
                <a href="@Url.Action("Details", "JobListing", new { id = job.id })">View Details of Job @job.id</a>

                </td>

            </tr>
        }
    </tbody>
</table>

